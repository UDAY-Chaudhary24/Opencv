import cv2
img= cv2.imread("pexels-jonaskakaroto-736230.jpg",)
img1=cv2.resize(img,(800,800))
gr=cv2.cvtColor(img1,cv2.COLOR_BGR2GRAY)
_,thresh=cv2.threshold(gr,120,255,cv2.THRESH_BINARY)
contores,hie=cv2.findContours(thresh,cv2.RETR_TREE,cv2.CHAIN_APPROX_SIMPLE)
cv2.drawContours(img1,contores,-1,(255,0,0),1)
cv2.imshow("hello",img1)
cv2.waitKey(0)
cv2.destroyAllWindows()
